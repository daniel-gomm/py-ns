    "paths": {
        "/v2": {
            "get": {
                "summary": "Stations V2",
                "description": "A list of stations that can be used to plan trips.",
                "operationId": "getStations",
                "parameters": [
                    {
                        "name": "q",
                        "in": "query",
                        "description": "A minimum 2 characters are required when this parameter is provided.",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "includeNonPlannableStations",
                        "in": "query",
                        "description": "Specify if you want to include stations that can't be used for planning a trip.",
                        "schema": {
                            "type": "boolean",
                            "default": false
                        }
                    },
                    {
                        "name": "countryCodes",
                        "in": "query",
                        "description": "A list of countryCodes to use as a filter.",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "example": "nl,d,b"
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "The limit is only applied when call is performed with q parameter. If invoked by countrycodes only, no limit is applied",
                        "schema": {
                            "type": "integer",
                            "default": 10
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A list of stations that can be used to plan trips",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/StationsV2Response"
                                },
                                "example": {
                                    "payload": [
                                        {
                                            "UICCode": "string",
                                            "UICCdCode": "string",
                                            "stationType": "STOPTREIN_STATION",
                                            "EVACode": "string",
                                            "cdCode": 0,
                                            "code": "string",
                                            "sporen": [
                                                {
                                                    "spoorNummer": "string"
                                                }
                                            ],
                                            "synoniemen": [
                                                "string"
                                            ],
                                            "heeftFaciliteiten": true,
                                            "heeftVertrektijden": true,
                                            "heeftReisassistentie": true,
                                            "namen": {
                                                "lang": "string",
                                                "middel": "string",
                                                "kort": "string",
                                                "festive": "string"
                                            },
                                            "land": "string",
                                            "lat": 0,
                                            "lng": 0,
                                            "radius": 0,
                                            "naderenRadius": 0,
                                            "distance": 0,
                                            "ingangsDatum": "string",
                                            "eindDatum": "string",
                                            "nearbyMeLocationId": {
                                                "value": "string",
                                                "type": "string"
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "example": {
                                    "message": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
