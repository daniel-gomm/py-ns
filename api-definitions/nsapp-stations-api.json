                    },
                    "heeftVertrektijden": {
                        "type": "boolean"
                    },
                    "heeftReisassistentie": {
                        "type": "boolean"
                    },
                    "namen": {
                        "$ref": "#/components/schemas/StationsNamen"
                    },
                    "land": {
                        "type": "string"
                    },
                    "lat": {
                        "type": "number"
                    },
                    "lng": {
                        "type": "number"
                    },
                    "radius": {
                        "type": "number"
                    },
                    "naderenRadius": {
                        "type": "number"
                    },
                    "distance": {
                        "type": "number"
                    },
                    "ingangsDatum": {
                        "type": "string"
                    },
                    "eindDatum": {
                        "type": "string"
                    },
                    "nearbyMeLocationId": {
                        "$ref": "#/components/schemas/NearbyMeLocationId"
                    }
                },
                "additionalProperties": false
            },
