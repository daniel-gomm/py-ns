                    "EVACode": {
                        "type": "string"
                    },
                    "cdCode": {
                        "type": "number"
                    },
                    "code": {
                        "type": "string"
                    },
                    "sporen": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Track"
                        }
                    },
                    "synoniemen": {
                        "type": "array",
                        "items": {
