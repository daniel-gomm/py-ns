                ],
                "type": "object",
                "properties": {
                    "spoorNummer": {
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "StationTypeV2": {
                "enum": [
                    "STOPTREIN_STATION",
                    "KNOOPPUNT_STOPTREIN_STATION",
                    "SNELTREIN_STATION",
                    "KNOOPPUNT_SNELTREIN_STATION",
                    "INTERCITY_STATION",
                    "KNOOPPUNT_INTERCITY_STATION",
                    "MEGA_STATION",
                    "FACULTATIEF_STATION"
                ],
                "type": "string"
            },
            "StationsNamen": {
